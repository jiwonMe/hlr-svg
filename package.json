{
  "name": "hlr",
  "version": "0.1.1",
  "description": "A TypeScript library for rendering 3D curves as SVG with Hidden Line Removal (HLR) / Hidden Curve Removal (HCR) support",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jiwonMe/hlr-svg.git"
  },
  "homepage": "https://github.com/jiwonMe/hlr-svg#readme",
  "bugs": {
    "url": "https://github.com/jiwonMe/hlr-svg/issues"
  },
  "keywords": [
    "svg",
    "3d",
    "hidden-line-removal",
    "hlr",
    "hcr",
    "bezier",
    "curves",
    "rendering"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "demo": "node dist/demo/main.js",
    "demo:all": "node dist/demo/main.js --all",
    "web:dev": "vite --config web/vite.config.ts",
    "web:build": "npm run build && vite build --config web/vite.config.ts",
    "web:preview": "vite preview --config web/vite.config.ts --port 4173"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.561.0",
    "motion": "^12.23.26",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.10.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^5.2.0",
    "shiki": "^3.20.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
